# -*- coding: utf-8 -*-
import math #引入math包
a=float(input("请输入a=")) #确定输入为数值类型
b=float(input("请输入b="))
c=float(input("请输入c="))
def quadratic(a,b,c):#定义一个有3个参数的函数
    p=b*b-4*a*c   
    if p>=0 and a!=0: #判断方式是否有解
        x1=(math.sqrt(p)-b)/(2*a)
        x2=(math.sqrt(p)+b)/(2*a)
        return x1,x2
    else:
        return ("方程无解")
print(quadratic(a,b,c,))
 
 #计算一元二次方程的解。
