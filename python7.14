sum=0
for x in range(101):
    sum=sum+x
    print(sum)
    
#注释：计算1+2+3+....+100，rang（101）为1-100的整数序列
//////////////////////////////////////////////////////
sum=0
n=99
while n>0:
    sum=sum+n
    n=n-2
    print(sum)
#100以内奇数和相加
    
sum=0
n=100
while n>0:
    sum=sum+n
    n=n-2
    print(sum)
#100以内偶数和相加
